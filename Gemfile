source "https://rubygems.org"

# Classy web-development dressed in a DSL (http://www.sinatrarb.com/)
gem "sinatra", "~> 1.4.0"
# Fast syntax highlighting for selected languages. (http://coderay.rubychan.de)
gem "coderay", "~> 1.1.1"
# Resque is a Redis-backed queueing system. (http://github.com/defunkt/resque)
gem "resque", "~> 1.22.0"
# resque-status is an extension to the resque queue system that provides simple trackable jobs. (http://github.com/quirkey/resque-status)
gem "resque-status", "~> 0.5.0"
# An asset pipeline implementation for Sinatra. (https://github.com/kalasjocke/sinatra-asset-pipeline)
gem "sinatra-asset-pipeline", "~> 0.7.0"
# Rake is a Make-like program implemented in Ruby (https://github.com/ruby/rake)
gem "rake", "~> 10.0"
# A powerful but elegant CSS compiler that makes CSS fun again. (http://sass-lang.com/)
gem "sass", "~> 3.4.22"
# Ruby wrapper for UglifyJS JavaScript compressor (http://github.com/lautis/uglifier)
gem "uglifier", "~> 2.7.1"
# Add comments to your Gemfile with each dependency's description. (https://github.com/ivantsepp/annotate_gem)
gem "annotate_gem"
# An IRB alternative and runtime developer console (http://pryrepl.org)
gem "pry"
